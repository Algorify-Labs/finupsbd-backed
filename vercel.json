{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node", 
        "config": {
        "buildCommand": "prisma generate && npm run build"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
        
      }
    ]
  }